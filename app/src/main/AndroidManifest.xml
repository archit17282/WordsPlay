<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.architpanwar.wordsplay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".enter" />
        <activity android:name=".play" />
        <activity
            android:name=".checknames"
            android:label="@string/title_activity_checknames"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FINAL"
            android:label="@string/title_activity_final"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".highscore"
            android:label="@string/title_activity_highscore"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".HELP"></activity>
    </application>

</manifest>